 debug=false,
 timer=0,
 timer_set=false,
 state="menu",
 winning_score=11,
 timer=0,
 interval=300,
 bg_color=colors.darkgreen,
 lines_color=colors.pink,
 theme="modern",
 menu ={
  --davide
  x=54,
  y=100,
  state="main",
  option_text={
